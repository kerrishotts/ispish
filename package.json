{
    "name": "ispish",
    "version": "0.0.1",
    "description": "A lisp/logo-like language",
    "main": "index.mjs",
    "bin": {
        "ispish": "./cli.mjs"
    },
    "scripts": {
        "test": "mocha-esm test/*.mjs",
        "cover": "nyc npm test",
        "lint": "eslint 'src/**/*.js' 'src/**/*.mjs'",
        "dev": "NODE_ENV=development webpack-dev-server --mode=development --hot"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kerrishotts/ispish.git"
    },
    "keywords": [
        "lisp",
        "logo",
        "parser",
        "javascript",
        "example"
    ],
    "author": "Kerri Shotts",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/kerrishotts/ispish/issues"
    },
    "homepage": "https://github.com/kerrishotts/ispish#readme",
    "dependencies": {
        "get-stdin": "^6.0.0",
        "monaco-editor": "^0.13.1",
        "pureimage": "^0.1.6",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-monaco-editor": "^0.17.2"
    },
    "devDependencies": {
        "@types/chai": "^4.1.3",
        "@types/mocha": "^5.2.0",
        "@types/node": "^10.1.2",
        "autoprefixer": "^8.6.5",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "chai": "^4.1.2",
        "clean-webpack-plugin": "^0.1.19",
        "css-loader": "^1.0.0",
        "esdoc": "^1.1.0",
        "esdoc-ecmascript-proposal-plugin": "^1.0.0",
        "esdoc-external-ecmascript-plugin": "^1.0.0",
        "esdoc-node": "^1.0.3",
        "esdoc-standard-plugin": "^1.0.0",
        "eslint": "^4.19.1",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-react": "^7.10.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "mocha": "^5.2.0",
        "mocha-esm": "^1.0.2",
        "monaco-editor-webpack-plugin": "^1.4.0",
        "nyc": "^11.8.0",
        "raw-loader": "^0.5.1",
        "react-markdown-loader": "^1.1.14",
        "style-loader": "^0.21.0",
        "webpack": "^4.16.0",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.4"
    },
    "nyc": {
        "check-coverage": true,
        "per-file": true,
        "lines": 75,
        "include": [
            "src/**/*.js",
            "src/**/*.mjs"
        ],
        "exclude": [
            "test/**/*.js"
        ],
        "reporter": [
            "lcov",
            "html",
            "text"
        ],
        "all": true,
        "report-dir": "./.coverage"
    },
    "esdoc": {
        "source": "./src",
        "destination": "./esdocs",
        "exclude": ".external-ecmascript.js",
        "plugins": [
            {
                "name": "esdoc-standard-plugin"
            },
            {
                "name": "esdoc-external-ecmascript-plugin",
                "option": {
                    "enable": false
                }
            },
            {
                "name": "esdoc-ecmascript-proposal-plugin",
                "option": {
                    "enable": false
                }
            },
            {
                "name": "esdoc-node"
            }
        ]
    }
}
