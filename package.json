{
    "name": "ispish",
    "version": "0.0.1",
    "description": "A lisp/logo-like language",
    "main": "index.mjs",
    "bin": {
        "ispish": "./cli.mjs"
    },
    "scripts": {
        "test": "mocha-esm test/*.mjs",
        "cover": "nyc npm test",
        "lint": "eslint 'src/**/*.js' 'src/**/*.mjs'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kerrishotts/ispish.git"
    },
    "keywords": [
        "lisp",
        "logo",
        "parser",
        "javascript",
        "example"
    ],
    "author": "Kerri Shotts",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/kerrishotts/ispish/issues"
    },
    "homepage": "https://github.com/kerrishotts/ispish#readme",
    "dependencies": {
        "get-stdin": "^6.0.0",
        "pureimage": "^0.1.6"
    },
    "devDependencies": {
        "@types/chai": "^4.1.3",
        "@types/mocha": "^5.2.0",
        "@types/node": "^10.1.2",
        "chai": "^4.1.2",
        "esdoc": "^1.1.0",
        "esdoc-ecmascript-proposal-plugin": "^1.0.0",
        "esdoc-external-ecmascript-plugin": "^1.0.0",
        "esdoc-node": "^1.0.3",
        "esdoc-standard-plugin": "^1.0.0",
        "eslint": "^4.19.1",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.12.0",
        "mocha": "^5.2.0",
        "mocha-esm": "^1.0.2",
        "nyc": "^11.8.0"
    },
    "nyc": {
        "check-coverage": true,
        "per-file": true,
        "lines": 75,
        "include": [
            "src/**/*.js",
            "src/**/*.mjs"
        ],
        "exclude": [
            "test/**/*.js"
        ],
        "reporter": [
            "lcov",
            "html",
            "text"
        ],
        "all": true,
        "report-dir": "./.coverage"
    },
    "esdoc": {
        "source": "./src",
        "destination": "./esdocs",
        "exclude": ".external-ecmascript.js",
        "plugins": [
            {
                "name": "esdoc-standard-plugin"
            },
            {
                "name": "esdoc-external-ecmascript-plugin",
                "option": {
                    "enable": false
                }
            },
            {
                "name": "esdoc-ecmascript-proposal-plugin",
                "option": {
                    "enable": false
                }
            },
            {
                "name": "esdoc-node"
            }
        ]
    }
}
