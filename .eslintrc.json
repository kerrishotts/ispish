{
    "extends": "airbnb-base",
    "rules": {
        "indent": [ "error", 4 ],
        "no-underscore-dangle": 0,
        "no-shadow": 0,
        "no-nested-ternary": 0,
        "no-param-reassign": 0,
        "global-require": 0
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ]
}
