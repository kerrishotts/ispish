let a 2
let b [ 10 20 30 ]
[
    "[10 20 30] . 1 should be 20"  (([10 20 30] . 1) == 20)
    "[10 20 30] . a should be 30"  (([10 20 30] . a) == 30)
    "b . a should also be 30"      ((b . a) == 30)
    "b . -1 should also be 30"     ((b . -1) == 30)
    "b . -2 should be 20"          ((b . -2) == 20)
    "size of b is 3"               ((size b) == 3)
]
